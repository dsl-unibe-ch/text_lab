---
batch_connect:
  template: "basic"


script:
  # Source these scripts before and after the main script
  before_file: "template/before.sh.erb"
  after_file: "template/after.sh"

  
  
  native:
    - "--account=<%= auto_accounts %>"
    - "--partition=<%= partition %>"
    - "--qos=<%= qos %>"
    - "--gres=<%= gpu %><%= gpu_number %>"
    - "--time=<%= time %>:00:00"
    - "--mem=60G"
    - "--cpus-per-task=4"
<% unless reservation.blank? %>
    - "--reservation=<%= reservation %>"
<% end %>
<% unless wckey.blank? %>
    - "--wckey=<%= wckey %>"
<% end %>

 
